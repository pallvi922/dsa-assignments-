#include<iostream>
using namespace std;

int main() {
    int m, n, p, t1, t2;
    int a[20][3], b[20][3], c[40][3];
    int k = 1;

    cin >> m >> n >> t1;
    for(int i = 0; i <= t1; i++)
        cin >> a[i][0] >> a[i][1] >> a[i][2];

    cin >> n >> p >> t2;
    for(int i = 0; i <= t2; i++)
        cin >> b[i][0] >> b[i][1] >> b[i][2];

    c[0][0] = m;
    c[0][1] = p;

    for(int i = 1; i <= t1; i++) {
        for(int j = 1; j <= t2; j++) {
            if(a[i][1] == b[j][0]) {
                int r   = a[i][0];
                int col = b[j][1];
                int val = a[i][2] * b[j][2];
                int f   = 0;

                for(int x = 1; x < k; x++) {
                    if(c[x][0] == r && c[x][1] == col) {
                        c[x][2] += val;
                        f = 1;
                    }
                }
                if(!f) {
                    c[k][0] = r;
                    c[k][1] = col;
                    c[k][2] = val;
                    k++;
                }
            }
        }
    }

    c[0][2] = k - 1;

    for(int i = 0; i < k; i++)
        cout << c[i][0] << " " << c[i][1] << " " << c[i][2] << endl;
}
