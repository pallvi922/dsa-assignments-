#include<iostream>
using namespace std;

int main() {
    int m, n, t;
    int a[20][3], b[20][3], k = 1;

    cin >> m >> n >> t;

    for(int i = 0; i <= t; i++)
        cin >> a[i][0] >> a[i][1] >> a[i][2];

    b[0][0] = n;
    b[0][1] = m;
    b[0][2] = t;

    for(int j = 0; j < n; j++) {
        for(int i = 1; i <= t; i++) {
            if(a[i][1] == j) {
                b[k][0] = a[i][1];
                b[k][1] = a[i][0];
                b[k][2] = a[i][2];
                k++;
            }
        }
    }

    for(int i = 0; i <= t; i++)
        cout << b[i][0] << " " << b[i][1] << " " << b[i][2] << endl;
}
