#include<iostream>
using namespace std;

int main() {
    int m, n, t1, t2;
    int a[20][3], b[20][3], c[40][3];
    int i = 1, j = 1, k = 1;

    cin >> m >> n >> t1;
    for(int x = 0; x <= t1; x++)
        cin >> a[x][0] >> a[x][1] >> a[x][2];

    cin >> m >> n >> t2;
    for(int x = 0; x <= t2; x++)
        cin >> b[x][0] >> b[x][1] >> b[x][2];

    c[0][0] = m;
    c[0][1] = n;

    while(i <= t1 && j <= t2) {
        if(a[i][0] == b[j][0] && a[i][1] == b[j][1]) {
            c[k][0] = a[i][0];
            c[k][1] = a[i][1];
            c[k][2] = a[i][2] + b[j][2];
            i++; j++;
        }
        else if(a[i][0] < b[j][0] || (a[i][0] == b[j][0] && a[i][1] < b[j][1])) {
            c[k][0] = a[i][0];
            c[k][1] = a[i][1];
            c[k][2] = a[i][2];
            i++;
        }
        else {
            c[k][0] = b[j][0];
            c[k][1] = b[j][1];
            c[k][2] = b[j][2];
            j++;
        }
        k++;
    }

    while(i <= t1) {
        c[k][0] = a[i][0];
        c[k][1] = a[i][1];
        c[k][2] = a[i][2];
        i++; k++;
    }

    while(j <= t2) {
        c[k][0] = b[j][0];
        c[k][1] = b[j][1];
        c[k][2] = b[j][2];
        j++; k++;
    }

    c[0][2] = k - 1;

    for(int x = 0; x < k; x++)
        cout << c[x][0] << " " << c[x][1] << " " << c[x][2] << endl;
}
