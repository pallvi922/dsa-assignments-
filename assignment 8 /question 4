#include <iostream>
#include <climits>
using namespace std;

struct Node{
    int data;
    Node* left;
    Node* right;
};

Node* create(int x){
    Node* n = new Node;
    n->data = x;
    n->left = n->right = NULL;
    return n;
}

bool isBST(Node* root, long long minv, long long maxv){
    if(root == NULL) return true;
    if(root->data <= minv || root->data >= maxv) return false;
    return isBST(root->left, minv, root->data) &&
           isBST(root->right, root->data, maxv);
}

int main(){
    Node* root = create(4);
    root->left = create(2);
    root->right = create(6);
    root->left->left = create(1);
    root->left->right = create(3);

    if(isBST(root, LLONG_MIN, LLONG_MAX))
        cout << "BST";
    else
        cout << "Not BST";
}
